pipeline {
  stages {
    stage ('Inicializacion') {
      steps {
        echo 'Inicializando pipeline'
      }
    }
    stage ('Pruebas Unitarias') {
      steps {
        echo 'Realizando Pruebas'
      }
    }
    stage ('Construccion') {
      steps {
        echo 'Construyendo imagen'
      }
    }
    stage ('Publicacion') {
      steps {
        echo 'Publicando imagen'
      }
    }
  }
}